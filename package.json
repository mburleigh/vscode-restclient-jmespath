{
	"name": "vscode-restclient-jmespath",
	"displayName": "vscode-restclient-jmespath",
	"description": "Add JMESPath support to vscode-restclient",
	"version": "0.0.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mburleigh/vscode-restclient-jmespath"
	},
	"author": {
		"name": "Matthew Burleigh",
		"email": "mburleigh@pobox.com"
	},
	"publisher": "mburleigh",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.runQuery",
				"title": "Run Query"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^5.0.4",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"applicationinsights": "^1.0.5",
		"fs-extra": "^5.0.0",
		"jmespath": "^0.15.0",
		"mpn": "^0.1.0"
	},
	"extensionDependencies": [
		"humao.rest-client"
	]
}